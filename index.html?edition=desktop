    <script>
      // for auto-adjusting the height of the iframe window
      var my_height_data;
      window.addEventListener('message', function(event) {
        // Ignores messages from untrusted domains.
        if (event.origin != 'https://go.neo4j.com' && event.origin != 'https://info.neo4j.com') return;
        my_height_data = event.data;
        if(my_height_data.indexOf('px') !== -1) {
           my_height_data = parseInt(my_height_data);
           $('iframe#mkto_iframe').height(my_height_data);
        }
      });
    </script>

  
 <!-- end of if is_singular etc -->

<!-- Send GA ClientID to Marketo iframe --> 
<script>
  jQuery('#mkto_iframe').load(function() {
    ga('create', 'UA-1192232-34', 'auto');
      ga(function(tracker) {
        var clientId = tracker.get('clientId');
        var frameWindow = document.getElementById('mkto_iframe').contentWindow;
        frameWindow.postMessage(clientId, 'https://go.neo4j.com');
      });
 });
</script>

